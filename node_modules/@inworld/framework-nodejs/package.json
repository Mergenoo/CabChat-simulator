{
  "name": "@inworld/framework-nodejs",
  "version": "0.3.1",
  "license": "SEE LICENSE IN LICENSE.md and LICENSE-CPP-BINARIES.md",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "inworld",
    "ai",
    "asr",
    "character",
    "npc",
    "speech recognition",
    "text to speech",
    "tts",
    "voice"
  ],
  "author": {
    "name": "Theai, Inc.",
    "url": "https://www.inworld.ai"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/inworld-ai/inworld-framework-nodejs.git"
  },
  "scripts": {
    "postinstall": "yarn binaries:unzip",
    "binaries:unzip": "node ./scripts/binaries-unzip.cjs",
    "build": "tsc -p .",
    "release:pack": "yarn build && node ./scripts/pack.cjs",
    "lint": "eslint \"{src,templates,__tests__}/**/*.{js,ts,tsx,jsx}\" --fix",
    "lint:check": "eslint \"{src,templates,__tests__}/**/*.{js,ts,tsx,jsx}\"",
    "prettier": "prettier --write \"{src,templates,__tests__}/**/*.{js,ts,tsx,jsx}\" --ignore-path .gitignore",
    "prettier:check": "prettier --check \"{src,templates,__tests__}/**/*.{js,ts,tsx,jsx}\" --ignore-path .gitignore",
    "test": "jest __tests__/package/* --config=jest.package.config.json  --no-cache",
    "templates:run": "jest __tests__/templates/* --config=jest.templates.config.json --no-cache",
    "test:debug": "node --inspect-brk jest __tests__/* --no-cache --runInBand",
    "test:coverage": "yarn test --coverage",
    "circular-deps": "madge --circular --extensions ts src",
    "prepare": "husky",
    "docs:generate-html": "typedoc --readme none --entryPointStrategy expand --out docs_html --disableSources --excludeInternal --excludeNotDocumented ./src",
    "docs:generate-mdx": "python3 ./scripts/html_to_mdx.py --output docs_mdx docs_html",
    "docs:generate": "yarn docs:generate-html && yarn docs:generate-mdx"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.14",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "allure-jest": "^3.2.2",
    "allure-js-commons": "^3.2.2",
    "archiver": "^7.0.1",
    "dotenv": "^16.4.7",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jest-junit": "^16.0.0",
    "madge": "^8.0.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.2.6",
    "typedoc": "^0.28.4",
    "typescript": "^5.8.2",
    "wav-decoder": "^1.3.0",
    "yargs": "^17.7.2"
  },
  "dependencies": {
    "@kaciras/deasync": "1.0.4",
    "deasync": "^0.1.30",
    "extract-zip": "^2.0.1",
    "koffi": "^2.10.1",
    "uuid": "^11.1.0"
  }
}
